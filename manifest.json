// JSON Document

{
	"name": "My First Extension",
	"version": "0.1",
	"description": "Build an Extension!",
	"manifest_version": 2,
	"browser_action": {
		"default_popup": "popup.html"
	},
	"options_page": "options.html",
	"content_scripts": [{
		"matches": [
			"<all_urls>"
		],
		"js": ["content.js", "jquery-3.3.1.min.js"]
	}]
}
